{
	"name": "@zilero/gulp-scss-compiler",
	"version": "1.0.1",
	"description": "A powerful Gulp plugin for SCSS optimization with embedded Sass compiler and advanced configuration options for optimal SCSS processing",
	"author": {
		"name": "Artemev Alexandr",
		"url": "https://github.com/Zilero232"
	},
	"license": "MIT",
	"homepage": "https://github.com/Zilero232/gulp-plugins-hub",
	"repository": {
		"type": "git",
		"url": "https://github.com/Zilero232/gulp-plugins-hub"
	},
	"bugs": {
		"url": "https://github.com/Zilero232/gulp-plugins-hub/issues"
	},
	"keywords": [
		"gulp",
		"gulp-plugin",
		"scss",
		"sass",
		"css",
		"compiler",
		"preprocessor",
		"embedded-sass",
		"sass-embedded",
		"sass",
		"css",
		"compiler"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf dist",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/**/*.ts"
	},
	"engines": {
		"node": ">= 16.0"
	},
	"dependencies": {
		"@zilero/gulp-plugin-factory": "workspace:*",
		"sass-embedded": "^1.89.2",
		"zod": "^3.25.76",
		"chalk": "5.4.1",
		"consola": "^3.4.2",
		"pretty-error": "^4.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"gulp": "^4.0.0"
	}
}
