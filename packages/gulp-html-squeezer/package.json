{
	"name": "@zilero/gulp-html-squeezer",
	"version": "1.0.2",
	"type": "module",
	"description": "A Gulp plugin for advanced HTML minification and optimization with Terser, supporting custom compression rules",
	"author": {
		"name": "Artemev Alexandr",
		"url": "https://github.com/Zilero232"
	},
	"license": "MIT",
	"homepage": "https://github.com/Zilero232/gulp-plugins-hub",
	"repository": {
		"type": "git",
		"url": "https://github.com/Zilero232/gulp-plugins-hub"
	},
	"bugs": {
		"url": "https://github.com/Zilero232/gulp-plugins-hub/issues"
	},
	"keywords": [],
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf dist",
		"type-check": "tsc --noEmit",
		"lint": "eslint src/**/*.ts"
	},
	"engines": {
		"node": ">= 16.0"
	},
	"dependencies": {
		"html-minifier-terser": "^7.2.0",
		"@zilero/gulp-plugin-factory": "workspace:*"
	},
	"devDependencies": {
		"@types/html-minifier-terser": "^7.0.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"gulp": "^4.0.0"
	}
}
